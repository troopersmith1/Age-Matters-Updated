<Patch>

    <Operation Class="PatchOperationSequence">
        <success>Always</success>
        <operations>

            <li Class="PatchOperationAdd">
                <xpath>/Defs/HediffGiverSetDef[
                    defName = "Human" or
                    defName = "WolverineX" or
                    defName = "OrganicKilhn" or
                    defName = "GhoulStandard" or
                    defName = "OrganicAvali" or
					defName = "O21_SG_JaffaStandard" or
					defName = "KroganHediffs" or
					defName = "HanarHediffs"
                    ]/hediffGivers</xpath>
                <value>
                    <li Class="HediffGiver_RandomAgeCurved">
                        <hediff>Youth</hediff>
                        <ageFractionMtbDaysCurve>
                            <points>
                                <li>(0, 0.00001)</li>
                                <li>(0.000001, 9999999999)</li>
                                <li>(0.6, 9999999999)</li>
                                <li>(1, 9999999999)</li>
                            </points>
                        </ageFractionMtbDaysCurve>
                    </li>
                </value>
            </li>
		</operations>
	</Operation>
			
	<!-- Bun Race -->		
			
			
    <Operation Class="PatchOperationSequence">
        <success>Always</success>
        <operations>
			<li Class="PatchOperationFindMod">
				<mods>
					<li>Bun Race</li>
				</mods>
				<match Class="PatchOperationSequence">
					<success>Always</success>
					<operations>
						<li Class="PatchOperationAdd">
							<xpath>/Defs/AlienRace.ThingDef_AlienRace[
								defName = "Alien_Bunlander" or
								defName = "Alien_BunTribe" or
								defName = "Alien_BunTribeNew"
								]</xpath>
							<value>
								<race/>
							</value>
						</li>                       
			
						<li Class="PatchOperationAdd">
							<xpath>/Defs/AlienRace.ThingDef_AlienRace[
								defName = "Alien_Bunlander" or
								defName = "Alien_BunTribe" or
								defName = "Alien_BunTribeNew"
								]/race</xpath>
							<value>
								<hediffGiverSets/>
							</value>
						</li>
			
						<li Class="PatchOperationAdd">
							<xpath>/Defs/AlienRace.ThingDef_AlienRace[
								defName = "Alien_Bunlander" or
								defName = "Alien_BunTribe" or
								defName = "Alien_BunTribeNew"
								]/race/hediffGiverSets</xpath>
							<value>
								<li>Human</li>
							</value>
						</li>
					</operations>
				</match>
			</li>
		</operations>
	</Operation>

			
